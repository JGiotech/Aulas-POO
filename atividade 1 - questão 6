public class questao6 {
    public static void main(String[] args){

        int A = 10;
        int B = 30;

        System.out.println("O valor original de A: " + A);
        System.out.println("O valor original de B: " + B);

        int variavel_auxiliar = A;
        A = B;
        B = variavel_auxiliar;

        System.out.println("O novo valor de A é: " + A);
        System.out.println("O novo valor de B é: " + B);

    }
}
